{
  "name": "taonado-cash",
  "version": "1.0.0",
  "description": "A Privacy Preserving TAO Mixer on Bittensor",
  "scripts": {
    "build": "hardhat compile",
    "deploy": "hardhat run scripts/deploy.ts",
    "deploy-local": "hardhat run scripts/deploy.ts --network local",
    "deploy-testnet": "hardhat run scripts/deploy.ts --network subevm",
    "test": "REPORT_GAS=true hardhat test --network hardhat",
    "miner": "hardhat run scripts/miner.ts",
    "miner-local": "hardhat run scripts/miner.ts --network local",
    "miner-testnet": "hardhat run scripts/miner.ts --network subevm",
    "vali": "hardhat run scripts/vali.ts",
    "vali-local": "hardhat run scripts/vali.ts --network local",
    "vali-testnet": "hardhat run scripts/vali.ts --network subevm",
    "deposit": "hardhat run scripts/deposit-cli.ts",
    "deposit-local": "hardhat run scripts/deposit-cli.ts --network local",
    "deposit-testnet": "hardhat run scripts/deposit-cli.ts --network subevm",
    "withdraw": "hardhat run scripts/withdrawal.ts",
    "withdraw-local": "hardhat run scripts/withdrawal.ts --network local",
    "withdraw-testnet": "hardhat run scripts/withdrawal.ts --network subevm",
    "balance": "hardhat run scripts/get-balance.ts",
    "balance-local": "hardhat run scripts/get-balance.ts --network local",
    "balance-testnet": "hardhat run scripts/get-balance.ts --network subevm",
    "transfer": "hardhat run scripts/transfer.ts",
    "transfer-local": "hardhat run scripts/transfer.ts --network local",
    "transfer-testnet": "hardhat run scripts/transfer.ts --network subevm"
  },
  "license": "MIT",
  "devDependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.7",
    "@nomicfoundation/hardhat-ethers": "^3.0.8",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.11",
    "@nomicfoundation/hardhat-toolbox": "^3.0.0",
    "@nomicfoundation/hardhat-verify": "^1.0.0",
    "@openzeppelin/contracts": "5.0.1",
    "@polkadot/keyring": "^13.4.4",
    "@polkadot/types": "^15.9.2",
    "@polkadot/util-crypto": "^13.4.4",
    "@typechain/ethers-v6": "^0.4.0",
    "@typechain/hardhat": "^8.0.0",
    "@types/chai": "^4.3.19",
    "@types/mocha": "^10.0.7",
    "@types/node": "^22.5.4",
    "chai": "^4.2.0",
    "ethers": "^6.13.2",
    "hardhat": "^2.19.4",
    "hardhat-gas-reporter": "^1.0.8",
    "node-json-db": "^2.3.1",
    "solidity-coverage": "^0.8.13",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3",
    "typechain": "^8.3.2",
    "typescript": "^5.3.3"
  },
  "bin": {
    "tnd": "./cli.ts"
  },
  "dependencies": {
    "commander": "^14.0.0"
  }
}
